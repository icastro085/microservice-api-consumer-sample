{
  "name": "delivery-consumer-response",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --inspect --ignore node_modules -r tsconfig-paths/register -r dotenv/config ./src/index.ts",
    "build": "rimraf ./build && tsc --project tsconfig-build.json && tscpaths -p tsconfig-build.json -s ./src -o ./build",
    "lint": "eslint --fix --ext .js,.ts,.json .",
    "test": "jest --config jest.config.ts"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1043.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "sqs-consumer": "^5.6.0"
  }
}
